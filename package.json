{
  "name": "livego-stream-viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "analyze": "vite build --mode production",
    "android": "npm run build && npx cap sync android && npx cap open android",
    "android:dev": "npx cap sync android && npx cap open android",
    "android:run": "npm run build && npx cap sync android && npx cap run android",
    "android:build": "npm run build && npx cap sync android && cd android && ./gradlew assembleDebug",
    "android:live": "concurrently \"npm run dev\" \"npm run android:dev\""
  },
  "dependencies": {
    "@capacitor/android": "^6.2.1",
    "@capacitor/cli": "^6.2.1",
    "@capacitor/core": "^6.2.1",
